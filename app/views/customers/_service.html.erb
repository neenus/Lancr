<tr>
  <td>
    <%= image_tag service.img.to_s, :class => "style_image" %>
  </td>
  <td>
    <h4><%= service.title %></h4>
    <br>
    <p><h4><%= service.description %></h4></p>
  </td>
  <td>
    <h4><%= service.start_time.time.strftime("From %D at %I:%m  %p") %></h4>

  </td>
  <td>

    <h4><%= service.end_time.time.strftime("To %D at %I:%m %p") %></h4>

  </td>
  <td>
    <h4>$<%= service.price %></h4>
  </td>
</tr>
